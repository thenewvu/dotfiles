[user]
  name = Vu Le
  email = thenewvu@gmail.com
[core]
  editor = nvim
[credential]
  helper = cache --timeout 360000
[gc]
  autoDetach = false
[pull]
  rebase = true
[diff]
  tool = vimdiff
[difftool]
  prompt = false
[difftool "vimdiff"]
  cmd = nvim -d $LOCAL $REMOTE
[merge]
  tool = vimdiff
[mergetool]
  prompt = false
[mergetool "vimdiff"]
  cmd = nvim -d $LOCAL $REMOTE $MERGED -c '$wincmd w' -c 'wincmd J'
